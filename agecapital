<script>
function switchTab(tab) {
  document.getElementById('page-web3').classList.remove('active');
  document.getElementById('page-web2').classList.remove('active');
  document.getElementById('tab-web3').classList.remove('active-web3');
  document.getElementById('tab-web2').classList.remove('active-web2');
  if (tab === 'web3') {
    document.getElementById('page-web3').classList.add('active');
    document.getElementById('tab-web3').classList.add('active-web3');
  } else {
    document.getElementById('page-web2').classList.add('active');
    document.getElementById('tab-web2').classList.add('active-web2');
  }
}
</script>
</body>
</html>
